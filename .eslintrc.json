{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": [
            "error",
            4
        ],
        "comma-dangle": "off",
        "arrow-parens": "off",
        "max-len": "warn",
        "prefer-const": "warn"
    }
}